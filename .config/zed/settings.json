{
    "auto_update": false,
    "show_edit_predictions": false,
    "telemetry": {
        "diagnostics": false,
        "metrics": false
    },

    "vim_mode": true,
    "vim": {
        "use_system_clipboard": "always",
        "highlight_on_yank_duration": 200
    },

    "theme": "Gruvbox Baby",
    "tab_size": 4,
    "buffer_line_height": { "custom": 1.4 },
    "buffer_font_family": "Iosevka Nerd Font Mono",
    "buffer_font_features": { "calt": false },
    "buffer_font_size": 22.0,
    "ui_font_size": 18,
    "agent_ui_font_size": 20.0,
    "agent_buffer_font_size": 20.0,
    "relative_line_numbers": "enabled",
    "cursor_blink": true,
    "cursor_shape": "bar",
    "colorize_brackets": true,
    "rounded_selection": false,
    "use_smartcase_search": true,
    "current_line_highlight": "none",

    "redact_private_values": true,
    "private_files": [
        "**/.env*",
        "**/*.pem",
        "**/*.key",
        "**/*.cert",
        "**/*.crt"
    ],

    "gutter": {
        "folds": false,
        "breakpoints": false,
        "runnables": false
    },

    "title_bar": {
        "show_sign_in": false,
        "show_branch_icon": true,
        "show_branch_name": true,
        "show_user_picture": false
    },

    "tabs": {
        "file_icons": true
    },

    "toolbar": {
        "quick_actions": false,
        "breadcrumbs": false
    },

    "scrollbar": {
        "cursors": false
    },

    "collaboration_panel": {
        "button": false
    },

    "notification_panel": {
        "button": false
    },

    "agent": {
        "default_profile": "ask",
        "default_model": {
            "provider": "openrouter",
            "model": "minimax/minimax-m2"
        },
        "always_allow_tool_actions": true,
        "model_parameters": []
    }
}

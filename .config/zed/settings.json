{
  "auto_update": false,
  "show_edit_predictions": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },

  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "always"
  },

  "tab_size": 2,
  "relative_line_numbers": "enabled",
  "ui_font_size": 18.0,
  "buffer_line_height": "standard",
  "buffer_font_size": 22.0,
  "buffer_font_family": "Iosevka Nerd Font",
  "agent_ui_font_size": 20.0,
  "theme": "Gruvbox Dark Hard",
  "cursor_blink": false,
  "cursor_shape": "bar",

  "redact_private_values": true,
  "private_files": [
    "**/.env*",
    "**/*.pem",
    "**/*.key",
    "**/*.cert",
    "**/*.crt",
    "**/secrets.yml"
  ],

  "tab_bar": {
    "show": true
  },
  "debugger": {
    "button": false
  },
  "status_bar": {
    "cursor_position_button": true,
    "active_language_button": true
  },
  "project_panel": {
    "button": true
  },
  "colorize_brackets": true,
  "gutter": {
    "folds": false,
    "breakpoints": false,
    "runnables": false
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "rounded_selection": false,
  "agent_buffer_font_size": 20.0,

  "agent": {
    "default_model": {
      "provider": "openrouter",
      "model": "kwaipilot/kat-coder-pro:free"
    },
    "always_allow_tool_actions": true,
    "model_parameters": []
  },

  "agent_servers": {
    "gemini": {
      "ignore_system_version": false
    }
  },

  "preview_tabs": {
    "enable_preview_from_file_finder": false
  },

  "title_bar": {
    "show_sign_in": false,
    "show_branch_icon": true,
    "show_branch_name": true,
    "show_user_picture": false
  },

  "tabs": {
    "git_status": false,
    "file_icons": true
  },

  "search": {
    "button": false,
    "center_on_match": true
  },

  "use_smartcase_search": true,
  "prettier": {
    "allowed": true
  },

  "toolbar": {
    "agent_review": true,
    "selections_menu": true,
    "quick_actions": false,
    "breadcrumbs": false
  },

  "scrollbar": {
    "show": "auto",
    "cursors": false
  },

  "collaboration_panel": {
    "button": false
  },

  "outline_panel": {
    "button": false
  },

  "notification_panel": {
    "button": false
  },

  "terminal": {
    "button": false
  }
}
